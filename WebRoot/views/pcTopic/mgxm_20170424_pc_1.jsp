    <script>
        $(function () {
            (function () {
                function loadImage() {
                    var imgContainers = $(".imgContainer");
                    var length = imgContainers.length;
                    var index = 0;
                    var item = null;
                    var imgUrl = null;
                    getImg();

                    function getImg() {
                        item = imgContainers.eq(index);
                        imgUrl = item.attr("imgUrl");
                        if (imgUrl != null || imgUrl != undefined) {
                            var image = new Image();
                            image.onload = function () {
                                item.css({
                                    "opacity": "1"
                                });
                                item.css({
                                    "background-image": "url(" + imgUrl + ")",
                                    "opacity": "1"
                                });
                                index++;
                                if (index < length) {
                                    getImg();
                                }
                            }
                            image.src = imgUrl;
                        } else {
                            index++;
                            if (index < length) {
                                getImg();
                            }
                        }
                    }
                }
                loadImage();
            })();
        });
        $('.one-time').slick({

            dots: false,

            infinite: true,

            speed: 300,

            slidesToShow: 3,

            touchMove: false,

            slidesToScroll: 1

        });
